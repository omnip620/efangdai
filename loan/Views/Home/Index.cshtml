    <style type="text/css">
        /* CSS Document */
        .lof-slidecontent { position: relative; overflow: hidden; border: #F4F4F4 solid 1px; width: 980px; height: 336px; margin: 0px auto; margin-top: 110px; }

            .lof-slidecontent .preload { height: 100%; width: 100%; background: transparent; position: absolute; top: 0; left: 0; z-index: 100000; color: #FFF; text-align: center; }

                .lof-slidecontent .preload div { height: 100%; width: 100%; background: transparent url(/Content/images/load-indicator.gif) no-repeat scroll 50% 50%; }

        .lof-main-outer { position: relative; height: 100%; width: 980px; z-index: 3; overflow: hidden; }

        /* main flash */
        ul.lof-main-wapper { /* margin-right:auto; */ background: transparent url(/Content/images/load-indicator.gif) no-repeat scroll 50% 50%; padding: 0px; margin: 0; height: 336px; width: 980px; position: absolute; overflow: hidden; }

            ul.lof-main-wapper li { overflow: hidden; padding: 0px; margin: 0px; height: 100%; width: 980px; float: left; }

        .lof-opacity li { position: absolute; top: 0; left: 0; float: inherit; }

        ul.lof-main-wapper li img { padding: 0px; width: 980px; height: 336px; }

        /* item navigator */
        ul.lof-navigator { top: 0; padding: 0; margin: 0; position: absolute; width: 100%; font-size: 12px; }

        .lof-navigator-outer { position: absolute; right: 0; top: 0px; z-index: 100; height: 336px; width: 130px; background: url(/Content/images/speedv2_banner_leftbg.png) repeat-y left; overflow: hidden; color: #999; }

        ul.lof-navigator li { cursor: pointer; list-style: none; width: 100%; height: 66px; padding: 0; margin: 0; overflow: hidden; }

        .lof-navigator li div { height: 100%; position: relative; padding-left: 8px; border-top: 1px solid #dfdfdf; }

            .lof-navigator li div p { margin-top: 15px; }

        .lof-navigator li img { height: 44px; width: 53px; margin: 15px 5px 10px 0px; float: left; }

        .lof-navigator li.active { background: url(/Content/images/speedv2_banner_left_activebg.png) no-repeat; color: #000; }

            .lof-navigator li.active div { }


        .orderbox { position: absolute; bottom: 0; right: 0; z-index: 999; height: 66px; width: 130px; border-top: 1px solid #dfdfdf; }

            .orderbox a { display: block; width: 130px; height: 55px; margin-top: 10px; font-size: 12px; }

                .orderbox a img { float: left; padding-left: 8px; }

                .orderbox a p { float: left; padding-left: 5px; margin-top: 10px; }

        .lof_shadow { background: url(/content/images/header_shadow.png) repeat-x; width: 980px; margin: 0px auto; height: 10px; position: relative; z-index: 9999; }

        .index_info { background: url(/content/images/index_info.jpg) no-repeat; height: 143px; width: 959px; margin: 0px auto; }
    </style>
}


