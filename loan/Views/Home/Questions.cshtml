@{
	
}
@section head{
	@{Html.RenderPartial("../shared/_css&js");}
}
<style type="text/css">
	.privacy { padding: 0 0 0 0px; }
</style>
<div id="wrapper">
    <div class="bread_crumbs"  style=" display:none;"><a href="http://www.efdai.com">易房贷</a>><a href="http://www.efdai.com">主页</a>><a href="http://www.efdai.com/questions.html">常见问题</a></div>
	<div id="container" class="cgstate">
		<div id="content">
			<div id="helps">
				@{Html.RenderPartial("../shared/_helpleft");}
				<div class="right">
					<div class="right_item">
						<div class="item_top"></div>
						<div class="h_icon1"></div>
						<div class="item_box">
							<h2 class="help_title">热门问题</h2>
							<div class="column privacy">
								<div class="privacy_item">

									@{System.Data.DataTable dt1 = ((System.Data.DataTable)ViewData["question"]);
								 
							 
							  
								}

								@if (dt1.Rows.Count < 1)
								{
									<li>无数据</li>
								}
								else
								{
									for (int i = 0; i < dt1.Rows.Count; i++)
									{ 
									<dl>
										
										<dt id="@dt1.Rows[i][0]">@dt1.Rows[i][1]</dt>
										<dd>@Html.Raw(@dt1.Rows[i][2].ToString())</dd>
									</dl>

									}
								}

									

								</div>

								<!-- #privacy -->
							</div>
						</div>

						<div class="item_bottom"></div>
					</div>
				</div>
				<!-- #helps -->
			</div>
			<!--#content -->
		</div>
		<!-- #container -->
	</div>
